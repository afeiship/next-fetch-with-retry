{
  "name": "vscode-gtc",
  "displayName": "vscode-gtc",
  "description": "Gtc extension for vscode.",
  "version": "1.0.0",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "publisher":"afeiship",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-gtc.gtc",
        "title": "GTC: Goto `GTC` command list."
      }
    ],
    "languages": [
      {
        "id": "json",
        "aliases": [
          "gtc",
          "gtcrc"
        ],
        "extensions": [
          ".gtc",
          ".gtcrc"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@jswork/node-gtc": "^1.0.2"
  }
}
