{
  "name": "vscode-gtc",
  "displayName": "vscode-gtc",
  "description": "Gtc extension for vscode.",
  "version": "1.0.19",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/afeiship/vscode-gtc"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "afeiship",
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:vscode-gtc.gtc",
    "onCommand:vscode-gtc.gtc:init"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-gtc.gtc",
        "title": "GTC: Goto `GTC` command list."
      },
      {
        "command": "vscode-gtc.gtc:init",
        "title": "GTC: Generate `.gtcrc` file."
      }
    ],
    "keybindings": [
      {
        "command": "vscode-gtc.gtc",
        "mac": "command+shift+t",
        "key": "ctrl+shift+t",
        "when": "true"
      }
    ],
    "languages": [
      {
        "id": "json",
        "aliases": [
          "gtc",
          "gtcrc"
        ],
        "extensions": [
          ".gtc",
          ".gtcrc"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@jswork/node-gtc": "^1.0.5",
    "@jswork/noop": "^1.0.0",
    "prettier": "^2.8.4"
  },
  "__metadata": {
    "id": "d3605352-ebeb-4f8b-80ce-f073853a6a85",
